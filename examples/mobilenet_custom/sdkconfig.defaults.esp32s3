# Default configurations for ESP32-S3

CONFIG_ESPTOOLPY_FLASHSIZE_8MB=y

CONFIG_ESP32S3_DEFAULT_CPU_FREQ_240=y
CONFIG_ESP32S3_SPIRAM_SUPPORT=y
CONFIG_SPIRAM_MODE_OCT=y

CONFIG_CAMERA_CORE1=y

CONFIG_ ESP32S3_INSTRUCTION_CACHE_32KB=y
CONFIG_ESP32S3_DATA_CACHE_64KB=y
CONFIG_ESP32S3_DATA_CACHE_8WAYS=y
CONFIG_ESP32S3_DATA_CACHE_LINE_64B=y

CONFIG_CAMERA_MODULE_ESP_S3_EYE=y
# CONFIG_ESPTOOLPY_NO_STUB=y
# CONFIG_ESP_CONSOLE_USB_SERIAL_JTAG=y

# display driver dettings
CONFIG_LV_COLOR_16_SWAP=y
CONFIG_LV_MEM_CUSTOM=y
CONFIG_LV_MEMCPY_MEMSET_STD=y
CONFIG_LV_USE_PERF_MONITOR=y
CONFIG_LV_SPRINTF_CUSTOM=y
